{
  "name": "remd",
  "version": "1.0.2",
  "description": "react markdown component",
  "keywords": [
    "react",
    "markdown",
    "component"
  ],
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack-doc-dev.config.js --open",
    "pub:doc": "NODE_ENV=production webpack --config webpack-doc-dist.config.js && rm -rf .doc",
    "pub:npm": "webpack --config webpack-npm.config.js && cp package.json .npm && npm publish .npm && rm -rf .npm"
  },
  "author": "gwuhaolin",
  "license": "ISC",
  "dependencies": {
    "marked": "^0.3.6",
    "prop-types": "^15.5.10",
    "react": "^15.5.4"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.2",
    "docgen-loader": "^1.3.4",
    "end-webpack-plugin": "^1.0.0",
    "file-loader": "^0.11.1",
    "gh-pages": "^1.0.0",
    "node-sass": "^4.5.3",
    "raw-loader": "^0.5.1",
    "react-docgen": "^3.0.0-beta4",
    "react-dom": "^15.5.4",
    "redemo": "^1.0.5",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.0",
    "web-webpack-plugin": "^1.5.8",
    "webpack": "^2.6.0",
    "webpack-dev-server": "^2.4.5",
    "webpack-node-externals": "^1.6.0"
  }
}
